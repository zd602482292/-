.container{
    margin-top: 30rpx;
    position:fixed;

}
.title {
    font-size:x-large;
    font-weight: bold;
    color: #00008B;
    margin-bottom: 10rpx;
}
.tab {
    float: left;
    width: 50%;
    text-align: center;
    padding: 10rpx 0;
  }
   
  .topTabSwiper {
    margin-top: 20rpx;
    border-bottom: 1px solid #ccc;
    zoom: 1;
  }
   
  .topTabSwiper:after {
    content: "";
    clear: both;
    display: block;
  }
   
  .tabBorer {
    border-bottom: 1px solid #00008B;
    color: #00008B;
  }

  .swiper {
    position: fixed;
    display: block;
    width: 100%;
    
  }

  .jilu{
    border-bottom: 1px solid #ccc;
    
  }
  .name{
    margin: 10rpx 10rpx;
    font-size: x-large;
    font-weight:bold
  }
  .time1{
    margin: 20rpx 10rpx;
    color:rgb(112, 112, 112);
    font-size: small;
  }
  .picker{
      border:1px solid black;
      width:20%
  }

.search {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 90rpx;
    background-color: #00008B;
  }
  .search .search_in {
    display: flex;
    align-items: center;
    justify-content:space-between;
    padding:0 20rpx ;
    box-sizing: border-box;
    height: 70rpx;
    width: 680rpx;
    background-color: #ffffff;
    border-radius: 35rpx;
  }
  .search_in image {
    height: 45rpx;
    width: 45rpx;
  }
  .search input {
    width: 480rpx;
  }
  .search_in .search_btn {
    height: 55rpx;
    width: 110rpx;
    border-radius: 55rpx;
    background-color: #00008B;
    color: #ffffff;
    text-align: center;
    line-height: 55rpx;
    cursor: pointer;
    padding: 0;
  }
  .swiper-items{
    height: 100%;
  }
  